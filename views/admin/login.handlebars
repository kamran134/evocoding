                <label class="form-label">
                    Şifrə
                </label>
                <input id="fpassword" type="password" class="form-control">
            </div>
            <button id="fconfirmbtn" class="w-100 btn btn-outline-info">Daxil ol</button>
        </div>
    </div>
    <script type="module" src="./src/javascript/pages/admin/{{filename}}.js"></script>
